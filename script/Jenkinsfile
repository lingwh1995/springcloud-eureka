//定义项目地址
def project_url = 'https://gitee.com/lingwh1995/springcloud-eureka.git'
node {
    def mvnHome
    agent { label '192.168.0.5'}      #指定在那台Jenkins节点上运行
    stage('检出代码') {
        echo '开始从git检出代码...'
        checkout([
            $class: 'GitSCM',
            branches: [[name: '*/master']], extensions: [],
            userRemoteConfigs: [[url: "${project_url}]]
        ])
        echo '完成从git检出代码...'
    }
    stage('Build') {
        echo 'setp 2222....'
    }
    stage('Results') {
        echo 'setp 3....'
    }
}